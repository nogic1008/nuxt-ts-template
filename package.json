{
  "name": "nuxt-ts-template",
  "version": "1.1.0",
  "description": "Template",
  "author": "nogic",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "fix": "run-p fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:prettier": "prettier --ignore-path .gitignore -l \"**/*.{json,yaml,yml}\"",
    "lint:tsc": "tsc --noEmit",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix",
    "*.{json,yaml,yml}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.3.9",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "buefy": "^0.8.12",
    "dotenv": "^8.2.0",
    "nuxt": "^2.11.0",
    "nuxt-buefy": "^0.3.23",
    "nuxt-property-decorator": "^2.5.1",
    "vuex-class-component": "^2.2.1"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.5.7",
    "@nuxtjs/eslint-config-typescript": "^1.0.2",
    "@nuxtjs/eslint-module": "^1.1.0",
    "@types/jest": "^25.1.3",
    "@vue/test-utils": "^1.0.0-beta.31",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-nuxt": ">=0.5.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "^10.0.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "vue-jest": "^4.0.0-0"
  }
}
