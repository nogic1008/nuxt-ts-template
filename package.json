{
  "name": "nuxt-ts-template",
  "version": "1.3.0",
  "description": "Template",
  "author": "nogic",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "bump": "npm version -m \"build: increment version to %s\"",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "fix": "run-p fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:prettier": "prettier --ignore-path .gitignore -l \"**/*.{json,yaml,yml}\"",
    "lint:tsc": "tsc --noEmit",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix",
    "*.{json,yaml,yml}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.4.6",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "buefy-helper-json": "buefy/buefy-helper-json",
    "dotenv": "^8.2.0",
    "flag-icon-css": "^3.4.6",
    "nuxt": "^2.12.2",
    "nuxt-buefy": "^0.3.29",
    "nuxt-i18n": "^6.11.1",
    "nuxt-property-decorator": "^2.7.2",
    "vuex-class-component": "^2.3.2"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.6.6",
    "@nuxtjs/eslint-config-typescript": "^1.0.2",
    "@types/jest": "^25.2.1",
    "@vue/test-utils": "^1.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.10.0",
    "eslint-plugin-nuxt": ">=0.5.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "^10.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "ts-jest": "^25.5.1",
    "vue-jest": "^4.0.0-0"
  }
}
