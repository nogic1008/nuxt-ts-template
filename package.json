{
  "name": "nuxt-ts-template",
  "version": "1.4.1",
  "description": "Template",
  "author": "nogic",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=10",
    "yarn": "^1.22.4"
  },
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "fix": "run-p fix:*",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:prettier": "prettier --ignore-path .gitignore -l \"**/*.{json,yaml,yml}\"",
    "lint:tsc": "tsc",
    "test": "jest"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/pwa": "^3.0.2",
    "flag-icon-css": "^3.5.0",
    "nuxt": "^2.14.5",
    "nuxt-buefy": "^0.4.3",
    "nuxt-i18n": "^6.15.0",
    "nuxt-property-decorator": "^2.7.2",
    "vuex-class-component": "^2.3.5"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.5",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/eslint-config-typescript": "^3.0.0",
    "@types/jest": "^25.2.3",
    "@vue/test-utils": "^1.1.0",
    "babel-jest": "^25.5.1",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "jest": "^25.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "ts-jest": "^25.5.1",
    "vue-jest": "^4.0.0-beta.5"
  }
}
