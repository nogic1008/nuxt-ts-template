{
  "name": "nuxt-ts-template",
  "version": "1.3.1",
  "description": "Template",
  "author": "nogic",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "bump": "npm version -m \"build: increment version to %s\"",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "fix": "run-p fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:prettier": "prettier --ignore-path .gitignore -l \"**/*.{json,yaml,yml}\"",
    "lint:tsc": "tsc --noEmit",
    "test": "jest"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.4.10",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "dotenv": "^8.2.0",
    "flag-icon-css": "^3.5.0",
    "nuxt": "^2.14.0",
    "nuxt-buefy": "^0.3.31",
    "nuxt-i18n": "^6.13.1",
    "nuxt-property-decorator": "^2.7.2",
    "vuex-class-component": "^2.3.5"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^1.0.3",
    "@nuxtjs/eslint-config-typescript": "^2.0.0",
    "@types/jest": "^25.2.3",
    "@vue/test-utils": "^1.0.3",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.19.0",
    "eslint-plugin-nuxt": ">=1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "jest": "^25.4.0",
    "jest-serializer-vue": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "ts-jest": "^25.4.0",
    "vue-jest": "^4.0.0-beta.3"
  }
}
